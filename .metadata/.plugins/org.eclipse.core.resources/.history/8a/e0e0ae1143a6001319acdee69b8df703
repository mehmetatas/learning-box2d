package com.turpgames.input;

public class KeyCommand {

	int keyCode;
	float x;
	float y;
	int pointer;
	int count;
	
	KeyCommand() {
		
	}
	
	public void reset() {
		
	}

	public int getKeyCode() {
		return keyCode;
	}

	public float getX() {
		return x;
	}

	public float getY() {
		return y;
	}

	public int getPointer() {
		return pointer;
	}

	public int getCount() {
		return count;
	}
}
