package com.turpgames.input;

public final class TapCommand extends InputCommand {
	private float x;
	private float y;
	private int count;
	
	TapCommand() {
		
	}
	
	void set(float x, float y, int count) {
		this.x = x;
		this.y = y;
		this.count = count;
		setAsUse();
	}

	public float getX() {
		return x;
	}

	public float getY() {
		return y;
	}

	public int getCount() {
		return count;
	}
}
