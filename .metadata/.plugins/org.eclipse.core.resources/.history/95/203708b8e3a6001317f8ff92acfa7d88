package com.turpgames.input;

abstract class KeyCommand extends InputCommand {
	int keyCode;
	
	class KeyUpCommand extends KeyCommand {
		@Override
		boolean execute(IInputListener listener) {
			return listener.keyUp(keyCode);
		}
	}
	
	class KeyDownCommand extends KeyCommand {
		@Override
		boolean execute(IInputListener listener) {
			return listener.keyUp(keyCode);
		}
	}
}
