package com.turpgames.input;

abstract class KeyCommand extends InputCommand {
	int keyCode;
	
	static class KeyUpCommand extends KeyCommand {
		@Override
		boolean execute(IInputListener listener) {
			return listener.keyUp(keyCode);
		}
	}
	
	static class KeyDownCommand extends KeyCommand {
		@Override
		boolean execute(IInputListener listener) {
			return listener.keyDown(keyCode);
		}
	}
}
