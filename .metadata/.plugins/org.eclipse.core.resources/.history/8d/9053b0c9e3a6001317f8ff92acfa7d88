package com.turpgames.input;

public final class LongPressCommand extends InputCommand {
	private float x;
	private float y;
	
	LongPressCommand() {
		
	}
	
	void set(float x, float y) {
		this.x = x;
		this.y = y;
		setAsUsed();
	}

	public float getX() {
		return x;
	}

	public float getY() {
		return y;
	}
}
